{
  "name": "PulseNet: AI Powered Disaster Prediction & Response Network",
  "short_name": "PulseNet",
  "description": "Enterprise-grade AI-powered disaster prediction and response network for Uttarakhand. Real-time landslide and flash flood monitoring with multi-language emergency alerts.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0A4A99",
  "theme_color": "#0A4A99",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en-US",
  "categories": ["emergency", "government", "utilities", "productivity"],
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "48x48",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any"
    }
  ],
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "540x720",
      "type": "image/svg+xml",
      "form_factor": "narrow",
      "label": "PulseNet Dashboard - Mobile View"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "1024x593",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "PulseNet Dashboard - Desktop View"
    }
  ],
  "shortcuts": [
    {
      "name": "Emergency Alert",
      "short_name": "Emergency",
      "description": "Send immediate emergency alert",
      "url": "/?action=emergency",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Live Map",
      "short_name": "Map",
      "description": "View real-time disaster map",
      "url": "/?tab=map",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Kedarnath 2013",
      "short_name": "History",
      "description": "View 2013 Kedarnath analysis",
      "url": "/?tab=kedarnath",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "web",
      "url": "https://pulsenet-disaster-ai.vercel.app"
    }
  ],
  "prefer_related_applications": false,
  "protocol_handlers": [
    {
      "protocol": "web+pulsenet",
      "url": "/?alert=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", ".pdf", ".doc", ".docx"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/files",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".gif"],
        "text/csv": [".csv"],
        "application/pdf": [".pdf"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": ["navigate-existing", "navigate-new"]
  },
  "handle_links": "preferred",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "permissions": [
    "geolocation",
    "notifications",
    "persistent-storage",
    "background-sync"
  ],
  "features": [
    "cross-origin-isolated",
    "offline-support",
    "real-time-updates",
    "push-notifications",
    "background-processing"
  ]
}